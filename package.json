{
	"name": "jsdoc-link",
	"displayName": "JSDoc Link",
	"description": "Preview JSDoc link in-place.",
	"version": "0.2.0",
	"publisher": "MuTsunTsai",
	"license": "MIT",
	"icon": "assets/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/MuTsunTsai/jsdoc-link"
	},
	"homepage": "https://github.com/MuTsunTsai/jsdoc-link",
	"bugs": "https://github.com/MuTsunTsai/jsdoc-link/issues",
	"keywords": [
		"jsdoc",
		"javascript",
		"typescript"
	],
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:vue",
		"onLanguage:svelte",
		"workspaceContains:*.js",
		"workspaceContains:*.jsx",
		"workspaceContains:*.ts",
		"workspaceContains:*.tsx",
		"workspaceContains:*.vue",
		"workspaceContains:*.svelte"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.170",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {}
}

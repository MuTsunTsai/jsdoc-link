{
	"name": "jsdoc-link",
	"displayName": "JSDoc Link",
	"description": "Preview JSDoc link in-place.",
	"version": "0.2.0",
	"publisher": "MuTsunTsai",
	"license": "MIT",
	"icon": "assets/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/MuTsunTsai/jsdoc-link"
	},
	"homepage": "https://github.com/MuTsunTsai/jsdoc-link",
	"bugs": "https://github.com/MuTsunTsai/jsdoc-link/issues",
	"keywords": [
		"jsdoc",
		"javascript",
		"typescript"
	],
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:vue",
		"onLanguage:svelte",
		"workspaceContains:*.js",
		"workspaceContains:*.jsx",
		"workspaceContains:*.ts",
		"workspaceContains:*.tsx",
		"workspaceContains:*.vue",
		"workspaceContains:*.svelte"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "pnpm compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "pnpm compile && pnpm lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.1",
		"@types/lodash": "^4.14.191",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.13.0",
		"@types/vscode": "^1.75.1",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"@vscode/test-electron": "^2.2.3",
		"eslint": "^8.34.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"typescript": "^4.9.5"
	}
}
